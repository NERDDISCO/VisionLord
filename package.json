{
  "name": "luminave",
  "version": "3.0.0",
  "description": "Manage all the lights with your web browser and create stunning visual experiences.",
  "main": "index.js",
  "author": "Tim Pietrusky",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NERDDISCO/luminave/issues"
  },
  "scripts": {
    "start": "concurrently --kill-others \"polymer serve\" \"npm run dekk-integration\" \"npm run modv-integration\"",
    "dekk-integration": "cd integration/dekk && npm start",
    "modv-integration": "cd integration/modV && npm start",
    "build": "npm run build:prpl-server && npm run build:static",
    "build:prpl-server": "polymer build --auto-base-path && gulp prpl-server",
    "build:static": "polymer build",
    "serve:prpl-server": "prpl-server --root server/build",
    "serve:static": "polymer serve --port 5000 build/esm-bundled",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "mocha test/integration --timeout=10000",
    "test:unit": "polymer test --module-resolution=node --npm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NERDDISCO/luminave.git"
  },
  "dependencies": {
    "@polymer/app-layout": "3.0.0-pre.25",
    "@polymer/iron-icons": "3.0.0-pre.25",
    "@polymer/iron-pages": "3.0.0-pre.25",
    "@polymer/lit-element": "^0.5.2",
    "@polymer/paper-button": "3.0.0-pre.25",
    "@polymer/paper-tabs": "3.0.0-pre.25",
    "@polymer/paper-tooltip": "3.0.0-pre.25",
    "@webcomponents/webcomponentsjs": "2.0.0",
    "concurrently": "^3.5.1",
    "fivetwelve": "^1.0.0-alpha.1",
    "immutability-helper": "github:nerddisco/immutability-helper",
    "keytime": "^0.1.1",
    "nodejs-websocket": "^1.7.1",
    "polymer-redux": "git+https://github.com/NERDDISCO/polymer-redux.git#feat/127/disableStateChangedEvent",
    "pwa-helpers": "^0.8.3",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "uuid": "^3.1.0",
    "webmidi": "^2.0.0",
    "webusb-dmx512-controller": "^1.0.6"
  },
  "homepage": "https://github.com/NERDDISCO/luminave#readme",
  "devDependencies": {
    "del": "^3.0.0",
    "eslint": "^4.7.2",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.2.3",
    "gulp-replace": "^1.0.0",
    "mocha": "^5.1.0",
    "prpl-server": "^1.1.0",
    "puppeteer": "^1.4.0",
    "wct-browser-legacy": "^1.0.1"
  }
}
